{
  "name": "friend_bot",
  "version": "1.0.0",
  "description": "Discord bot for a memey server",
  "scripts": {
    "build": "tsc",
    "start:autoreload": "nodemon --watch 'src/**/*.ts' --exec \"npm start\"",
    "start": "npm run build && node dist/index.js",
    "test": "npx jest",
    "lint": "eslint ./src --ext .ts --quiet --fix",
    "lint-nofix": "eslint ./src --ext .ts --quiet",
    "docs": "typedoc --mode file --excludeNotExported --excludePrivate --includeDeclarations --excludeExternals --out docs src --theme node_modules/eledoc/bin/default/"
  },
  "author": "A group of friends",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "bufferutil": "^4.0.1",
    "discord.js": "github:discordjs/discord.js",
    "erlpack": "github:discordapp/erlpack",
    "ffmpeg-static": "^4.2.7",
    "libsodium-wrappers": "^0.7.4",
    "luxon": "^1.25.0",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "tsyringe": "^4.3.0",
    "utf-8-validate": "^5.0.2",
    "zlib-sync": "^0.1.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/luxon": "^1.25.0",
    "@types/node": "^12.12.7",
    "@types/redis": "^2.8.28",
    "@types/sqlite3": "^3.1.6",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "eledoc": "^0.2.1",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "nodemon": "^2.0.5",
    "prettier": "^2.1.2",
    "source-map-support": "^0.5.16",
    "ts-jest": "^26.4.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
}
